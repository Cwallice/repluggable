{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Current File",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "neverOpen",
      "program": "${workspaceRoot}/node_modules/.bin/jest",
      "args": [
        "${relativeFile}"
      ]
    }
  ]
}
