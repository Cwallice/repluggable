{
  "extends": "tslint-config-yoshi",
  "rules": {
    "switch-default": false,
    "use-default-type-parameter": false,
    "no-floating-promises": false,
    "prettier": [true, {
      "printWidth": 140,
      "trailingComma": "none",
      "tabWidth": 4,
      "semi": false,
      "singleQuote": true
    }]
  }
}
