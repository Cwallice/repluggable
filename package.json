{
  "name": "@wix/repluggable",
  "version": "1.300.0",
  "license": "MIT",
  "description": "Allows composition of a React-with-Redux application entirely from a list of pluggable packages",
  "main": "dist/src/index.js",
  "module": "dist/es/src/index.js",
  "unpkg": true,
  "author": {
    "name": "Responsive FEDs",
    "email": "responsive-feds@wix.com"
  },
  "publishConfig": {
    "registry": "http://npm.dev.wixpress.com/",
    "versionBumpStrategy": "patch"
  },
  "files": [
    "*"
  ],
  "scripts": {
    "start": "yoshi start --no-server",
    "build": "yoshi build",
    "test": "yoshi test --coverage",
    "posttest": "yoshi lint",
    "test:performance": "perfer"
  },
  "dependencies": {
    "lodash": "^4.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^7.0.0",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.0.0",
    "@types/jest": "^25.0.0",
    "@types/lodash": "^4.0.0",
    "@types/react": "^16.0.0",
    "@types/react-redux": "^7.0.0",
    "@wix/perfer": "^1.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "husky": "^4.0.0",
    "jest-yoshi-preset": "^4.0.0",
    "lint-staged": "^10.0.0",
    "typescript": "^3.0.0",
    "yoshi": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yoshi lint --fix true"
    ]
  },
  "jest": {
    "preset": "jest-yoshi-preset"
  },
  "perfer": {
    "bundleSize": {
      "files": [
        "./dist/statics/repluggable.bundle.js",
        {
          "glob": "./dist/statics/repluggable.bundle.js",
          "maxSize": "15kb"
        }
      ]
    },
    "features": {
      "slackChannel": "editorx-important"
    }
  },
  "yoshi": {
    "hmr": true,
    "exports": "repluggable",
    "entry": {
      "repluggable": "index"
    },
    "externals": {
      "react": "React",
      "react-dom": "ReactDOM",
      "lodash": "_",
      "react-redux": "ReactRedux",
      "redux": "Redux"
    },
    "servers": {
      "cdn": {
        "port": 3201
      }
    }
  }
}
